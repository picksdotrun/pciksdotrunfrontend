VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_LAUNCH_WALLET= # optional public address to display and to fetch balance for UI; can also use ?wallet=ADDRESS
AUTH_JWT_SECRET=replace_with_strong_secret

# Privy (frontend app ID for SDK)
VITE_PRIVY_APP_ID=your_privy_app_id

# Serverless (Netlify Function) environment variables
# Set these in Netlify UI or a server-only .env, NOT exposed to client
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
XAI_API_KEY=your_xai_grok_api_key # required for grok-judge
# Optional: overrides (defaults baked into code)
# XAI_MODEL=grok-4-fast
# XAI_TIMEOUT_MS=60000
RPC_URL=https://api.mainnet-beta.solana.com
# Private key for launching tokens when no user dev key is available
DEV_WALLET_PRIVATE_KEY=base58_or_base64_or_json64_secret_key
# Optional: public key for wallet balance display (if set, avoids deriving from private key)
DEV_WALLET_PUBLIC_KEY=your_wallet_public_key
SOLANA_TRACKER_API_KEY=your_solana_tracker_key
# Solana Tracker API base URL
SOLANA_TRACKER_BASE_URL=https://pro-api.solanatracker.io
BACKEND_URL=https://your-inkwell-backend.example.com
BACKEND_USER_ID=54ea1ba1-69f1-4a75-9c85-b123d67b5f05
BACKEND_USER_PRIVATE_KEY= # optional; base64/base58/JSON(64) if backend user's dev wallet is null
SOLANA_TRACKER_SWAP_BASE_URL=https://swap-v2.solanatracker.io

# Privy server verification (optional; enables verifying Privy access tokens server-side)
PRIVY_APP_ID=your_privy_app_id
PRIVY_APP_SECRET=your_privy_app_secret

# Optional: frontend RPC for confirmations
VITE_RPC_URL=https://api.mainnet-beta.solana.com
VITE_SOLANA_TRACKER_SWAP_BASE_URL=https://swap-v2.solanatracker.io
VITE_SOLANA_TRACKER_API_KEY=
VITE_JUPITER_BASE_URL=https://quote-api.jup.ag
VITE_BACKEND_URL=https://your-evm-backend.example.com

# xAI / Grok (Serverless-only)
# Set these in Netlify (or your server env). Do NOT expose the API key to the client.
XAI_API_KEY=your_xai_api_key
XAI_API_BASE=https://api.x.ai
# Optional default model used by the serverless proxy
XAI_DEFAULT_MODEL=grok-beta
# Optional: override backend endpoint used by grok-judge to resolve EVM markets
# EVM_RESOLVE_BACKEND_URL=https://picksbackend-production.up.railway.app
# EVM_RESOLVE_BACKEND_KEY=shared_secret_if_backend_requires_it

# Optional: frontend control (no secrets here)
# Path to the Netlify function used as proxy for Grok
VITE_GROK_FUNCTION_PATH=/.netlify/functions/grok

# Optional override; default points to https://picksbackend-production.up.railway.app
VITE_API_BASE_URL=
